{
  "name": "clinical-kpi-design",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ts/libs/shared/ui/clinical-kpi-design/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:ui"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/ts/libs/shared/ui/clinical-kpi-design",
        "main": "ts/libs/shared/ui/clinical-kpi-design/src/index.ts",
        "tsConfig": "ts/libs/shared/ui/clinical-kpi-design/tsconfig.lib.json",
        "assets": ["ts/libs/shared/ui/clinical-kpi-design/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "ts/libs/shared/ui/clinical-kpi-design/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "ts/libs/shared/ui/clinical-kpi-design/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "ts/libs/shared/ui/clinical-kpi-design/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/clinical-kpi-design",
        "configDir": "ts/libs/shared/ui/clinical-kpi-design/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "dependsOn": [
        {
          "target": "build"
        }
      ],
      "options": {
        "cwd": "dist/ts/libs/shared/ui/clinical-kpi-design",
        "command": "npm publish"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.lib.json --pretty --noEmit",
            "forwardAllArgs": false
          },
          {
            "command": "tsc -p tsconfig.spec.json --pretty --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "ts/libs/shared/ui/clinical-kpi-design"
      }
    }
  }
}
