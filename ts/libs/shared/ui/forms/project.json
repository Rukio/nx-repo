{
  "name": "shared-ui-forms",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ts/libs/shared/ui/forms/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["ts/libs/shared/ui/forms/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../../coverage/ts/libs/shared/ui/forms"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.lib.json --pretty --noEmit",
            "forwardAllArgs": false
          },
          {
            "command": "tsc -p tsconfig.spec.json --pretty --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "ts/libs/shared/ui/forms"
      }
    }
  },
  "tags": ["type:ui", "scope:shared"]
}
