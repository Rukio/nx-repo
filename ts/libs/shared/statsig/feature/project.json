{
  "name": "shared-statsig-feature",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ts/libs/shared/statsig/feature/src",
  "projectType": "library",
  "tags": ["type:feature", "scope:shared"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "ts/libs/shared/statsig/feature/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../../coverage/ts/libs/shared/statsig/feature"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.lib.json --pretty --noEmit",
            "forwardAllArgs": false
          },
          {
            "command": "tsc -p tsconfig.spec.json --pretty --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "ts/libs/shared/statsig/feature"
      }
    }
  }
}
