{
  "name": "nest-statsig",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ts/libs/nest/statsig/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["ts/libs/nest/statsig/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/ts/libs/nest/statsig"],
      "options": {
        "jestConfig": "ts/libs/nest/statsig/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.lib.json --pretty --noEmit",
            "forwardAllArgs": false
          },
          {
            "command": "tsc -p tsconfig.spec.json --pretty --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "ts/libs/nest/statsig"
      }
    }
  },
  "tags": ["scope:shared", "type:nest"]
}
