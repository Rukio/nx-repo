{
  "name": "patient-portal-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ts/libs/patient-portal/feature/src",
  "projectType": "library",
  "tags": ["type:feature", "scope:patient-portal"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "ts/libs/patient-portal/feature/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../coverage/ts/libs/patient-portal/feature"
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.lib.json --pretty --noEmit",
            "forwardAllArgs": false
          },
          {
            "command": "tsc -p tsconfig.spec.json --pretty --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "ts/libs/patient-portal/feature"
      }
    }
  }
}
