{
  "name": "caremanager-feature-visit",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ts/libs/caremanager/feature-visit/src",
  "projectType": "library",
  "tags": ["type:feature"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "ts/libs/caremanager/feature-visit/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/ts/libs/caremanager/feature-visit"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../coverage/ts/libs/caremanager/feature-visit"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.lib.json --pretty --noEmit",
            "forwardAllArgs": false
          },
          {
            "command": "tsc -p tsconfig.spec.json --pretty --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "ts/libs/caremanager/feature-visit"
      }
    }
  }
}
