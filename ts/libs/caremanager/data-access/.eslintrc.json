{
  "extends": [
    "plugin:promise/recommended",
    "plugin:vitest/recommended",
    "../../../../.eslintrc.react.json"
  ],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "parserOptions": {
        "project": ["ts/libs/caremanager/data-access/tsconfig.*?.json"]
      },
      "rules": {
        "@typescript-eslint/no-floating-promises": "error"
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
